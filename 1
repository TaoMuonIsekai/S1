local function UpdatePlayerList()
local plrlist = {}
for i, v in pairs(game.Players:GetPlayers()) do
table.insert(plrlist, v.Name)
end
return plrlist
end

local dr = Tab:AddDropdown({Name = "NgÆ°á»i ChÆ¡i", Default = "1", Options = UpdatePlayerList(), Callback = function(v)
print(v)
getgenv().tele = v
end})

local function RefreshPlayerList()
local newPlrList = UpdatePlayerList()
dr:Refresh(newPlrList, true)
end
spawn(function()
while wait(5) do
RefreshPlayerList()
end
end)
